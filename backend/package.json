    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "tsc --project tsconfig.json",
    "start:local": "env-cmd -f .env.local -- node -r tsconfig-paths/register dist/app.js",
    "start:dev": "env-cmd -f .env.development -- node -r tsconfig-paths/register dist/app.js",
    "start:prod": "env-cmd -f .env.production -- node -r tsconfig-paths/register dist/app.js"
