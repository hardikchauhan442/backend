    "local": "env-cmd -f .env.local  nodemon --exec \"node --max-old-space-size=6144 --gc-interval=100 --expose-gc -r tsconfig-paths/register -r ts-node/register\" src/app.ts",
    "dev": "env-cmd -f .env.development  nodemon --exec \"node --max-old-space-size=6144 --gc-interval=100 --expose-gc -r tsconfig-paths/register -r ts-node/register\" src/app.ts",
